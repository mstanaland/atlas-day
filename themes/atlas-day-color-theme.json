	"tokenColors": [
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#009C77"
			}
		},
		{
			"name": "String Regexp",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#00875A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable",
				"variable.object.property",
				"variable.parameter",
				"variable.interpolation"
			],
			"settings": {
				"foreground": "#3175DC"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#3175DC"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"keyword.control",
				"storage"
			],
			"settings": {
				"foreground": "#42526E"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
